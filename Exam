SEND "What is the temperature" TO DISPLAY
RECEIVE Temperature FROM KEYBOARD
IF Temperature <18 THEN
SEND "COLD, the prefect teamperature for sleep is 18-21 degrees." TO DISPLAY
ELSE
IF Temperture > 21 THEN
SEND "PREFECT" TO DISPLAY
ELSE 
SEND "No!, the prefect temperature for sleep is 18-21 degrees." TO DISPLAY
END IF 
END IF 
